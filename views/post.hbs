	<script src="/pub/js/vendor/scrolloverflow.min.js"></script>
	<script src="/pub/js/vendor/all.js"></script>
	<script src="/pub/js/particlejs/particles.min.js"></script>

	<!-- Countdown script -->
	<script src="/pub/js/jquery.downCount.js"></script>

	<!-- Form script -->
	<script src="/pub/js/form_script.js"></script>

	<!-- Javascript main files -->
	<script src="/pub/js/main.js"></script>

